{
  "module": {
    "name": "entry",
    "type": "entry",
    "description": "$string:module_desc",
    "srcEntry": "./ets/stage/AbilityStage.ets",
    "mainElement": "EntryAbility",
    "deviceTypes": [
      "phone",
      "tablet",
      "2in1"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "metadata": [
      {
        "name": "ArkTSPartialUpdate",
        "value": "true"
      }
    ],
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "EntryAbility",
        "srcEntry": "./ets/ability/entryability/EntryAbility.ets",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:EntryAbility_desc",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ]
      },
      {
        "name": "WebPageAbility",
        "srcEntry": "./ets/ability/webpageAbility/WebPageAbility.ets",
        "description": "$string:WebPageAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:WebPageAbility_desc",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true
      },
      {
        "name": "FruitDetailAbility",
        "srcEntry": "./ets/ability/fruitDetailAbility/FruitDetailAbility.ets",
        "description": "$string:FruitDetailAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:FruitDetailAbility_desc",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "launchType": "specified"
      }
    ]
  }
}
